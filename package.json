{
  "name": "remark-link-card-plus",
  "version": "0.4.1",
  "description": "Remark plugin to convert text links to link cards",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "check": "biome check ./src",
    "check:fix": "biome check --write --unsafe ./src",
    "check:ci": "biome ci ./src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okaryo/remark-link-card-plus.git"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "markdown",
    "link",
    "card"
  ],
  "author": "okaryo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okaryo/remark-link-card-plus/issues"
  },
  "homepage": "https://github.com/okaryo/remark-link-card-plus#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@remark-embedder/core": "^3.0.3",
    "@types/node": "^22.13.5",
    "@types/sanitize-html": "^2.15.0",
    "msw": "^2.7.3",
    "remark": "^15.0.1",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "file-type": "^20.4.1",
    "open-graph-scraper": "^6.9.0",
    "sanitize-html": "^2.15.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  }
}
